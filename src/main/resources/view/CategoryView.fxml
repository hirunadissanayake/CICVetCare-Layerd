            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" layoutX="658.0" layoutY="195.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" style="-fx-background-color: #27ae60;" text="Delete Category" textFill="WHITE">
