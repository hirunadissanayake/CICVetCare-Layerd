            <TableColumn fx:id="colCategoryId" prefWidth="300.0" text="Category_Id" />
            <TableColumn fx:id="colName" prefWidth="456.0" text="Name" />
         </columns>
      </TableView>
      <Text fill="#1b1464" layoutX="60.0" layoutY="132.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category Id">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Text>
      <Text fill="#1b1464" layoutX="438.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font name="System Bold" size="17.0" />
